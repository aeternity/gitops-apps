app:
  image:
    tag: "latest"
    repository: "aeternitybot/state-channel-demo-frontend"
    pullPolicy: "Always"
  ingress:
    enabled: "true"
    hosts:
      - host: "state-channel-demo.stg.aepps.com"
        paths:
          - path: "/"
      - host: "develop-state-channel-demo-backend.prd.aepps.com"
        paths:
          - path: "/"
            version: "develop"
      - host: "pr-37-state-channel-demo-frontend.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-37"
      - host: "pr-43-state-channel-demo-frontend.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-43"
  probes:
    liveness:
      enabled: "false"
    readiness:
      enabled: "false"
  versions:
    - version: "pr-37"
    - version: "pr-27"
      gitSha: "ccf4f4b"
    - version: "pr-43"
    - {version: develop, gitSha: ''}
    - {version: pr-50, gitSha: 38b006b}
    - '{version: pr-49, gitSha: 9074948}'
