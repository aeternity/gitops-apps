app:
  image:
    tag: "latest"
    repository: "aeternitybot/state-channel-demo-frontend"
    pullPolicy: "Always"
  ingress:
    enabled: "true"
    hosts:
      - host: "state-channel-demo.dev.aepps.com"
        paths:
          - path: "/"
  probes:
    liveness:
      enabled: false
    readiness:
      enabled: false
  versions:
    - '{version: develop, gitSha: }'
