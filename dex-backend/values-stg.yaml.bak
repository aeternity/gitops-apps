app:
  image:
    repository: "aeternitybot/dex-backend"
    tag: "latest"
    pullPolicy: "Always"
  encValues:
    POSTGRES_PASSWORD: ""
  service:
    port: "3000"
  ingress:
    enabled: "true"
    hosts:
      - host: "dex-backend.stg.aepps.com"
        paths:
          - path: "/"
      - host: "pr-56-dex-backend.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-56"
  init:
    enabled: "true"
    command: "/bin/sh"
    args:
      - "-c"
      - "rm -rf .env && export DATABASE_URL=postgresql://$POSTGRES_USERNAME:$POSTGRES_PASSWORD@$POSTGRES_HOST:$POSTGRES_PORT/$POSTGRES_DATABASE?schema=$POSTGRES_SCHEMA && npx prisma db push --accept-data-loss"
  env:
    "POSTGRES_DB": "dex-backend-app"
    "POSTGRES_SCHEMA": "dex-backend-app" # will be overrided by the template with version name
    "POSTGRES_PORT": "5432"
    "POSTGRES_HOST": "dexstg.cxhy2i9at0vj.eu-central-1.rds.amazonaws.com"
    "POSTGRES_USERNAME": "postgres"
    "NETWORK_NAME": "testnet"
    "ROUTER_ADDRESS": "ct_MLXQEP12MBn99HL6WDaiTqDbG4bJQ3Q9Bzr57oLfvEkghvpFb"
    "FACTORY_ADDRESS": "ct_NhbxN8wg8NLkGuzwRNDQhMDKSKBwDAQgxQawK7tkigi2aC7i9"
  versions:
    - {version: pr-56, gitSha: f0c8689}
    - '{version: pr-46, gitSha: c43300e}'
