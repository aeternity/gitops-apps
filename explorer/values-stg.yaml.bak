app:
  image:
    tag: "latest"
    repository: "aeternitybot/explorer-frontend"
    pullPolicy: "Always"
  ingress:
    enabled: "true"
    hosts:
      - host: "explorer.stg.aepps.com"
        paths:
          - path: "/"
      - host: "pr-15-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-15"
      - host: "pr-16-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-16"
  versions:
    - {version: pr-16, gitSha: a766b7e}
    - {version: pr-13, gitSha: df1d629}
    - {version: pr-15, gitSha: c832ab5}
    - '{version: pr-22, gitSha: 03464d9}'
