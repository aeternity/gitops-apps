app:
  image:
    tag: "latest"
    repository: "aeternitybot/explorer-frontend"
    pullPolicy: "Always"
  ingress:
    enabled: "true"
    hosts:
      - host: "explorer.stg.aepps.com"
        paths:
          - path: "/"
      - host: "pr-6-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-6"
      - host: "pr-7-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-7"
      - host: "pr-8-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-8"
  versions:
    - version: "pr-7"
      gitSha: "cd885d2"
    - {version: pr-8, gitSha: 4314d51}
    - '{version: pr-6, gitSha: 93d5acd}'
