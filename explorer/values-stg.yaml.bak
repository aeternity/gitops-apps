app:
  image:
    tag: "latest"
    repository: "aeternitybot/explorer-frontend"
    pullPolicy: "Always"
  ingress:
    enabled: "true"
    hosts:
      - host: "explorer.stg.aepps.com"
        paths:
          - path: "/"
      - host: "pr-41-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-41"
      - host: "pr-44-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-44"
      - host: "pr-45-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-45"
  versions:
    - version: "pr-13"
      gitSha: "df1d629"
    - version: "pr-31"
      gitSha: "700e528"
    - version: "pr-34"
      gitSha: "c85c00b"
    - version: "pr-37"
      gitSha: "32ca07f"
    - version: "pr-41"
      gitSha: "11f2c24"
    - version: "pr-45"
    - '{version: pr-44, gitSha: 9c7532e}'
