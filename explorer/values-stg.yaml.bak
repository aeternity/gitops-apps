app:
  image:
    tag: "latest"
    repository: "aeternitybot/explorer-frontend"
    pullPolicy: "Always"
  ingress:
    enabled: "true"
    hosts:
      - host: "explorer.stg.aepps.com"
        paths:
          - path: "/"
      - host: "pr-13-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-13"
      - host: "pr-15-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-15"
      - host: "pr-16-explorer.stg.aepps.com"
        paths:
          - path: "/"
            version: "pr-16"
  versions:
    - version: "pr-15"
      gitSha: "e84711d"
    - {version: pr-13, gitSha: 843c9f0}
    - '{version: pr-16, gitSha: f667aa5}'
